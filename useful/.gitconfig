[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = jiedou
	email = jixfly@163.com
[alias]
	st=status
	pb=pull --rebase 
	pl=pull
	ct=commit
	ph=push
	rb=rebase
	br=branch
	ck=checkout
	df=diff
	lg=log
	sb=submodule update --init
	
[core]
	ui=color
#	editor=notepad
	editor=vim
[color]
	status=auto
	diff=auto
	branch=auto
	interactive=auto
	ui=true
